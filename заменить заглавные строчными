using System;
class HelloWorld {
  static void Main() {
    Console.Write("Введите строку: ");
    string str = Console.ReadLine();
    string newstr = "";
    for (int i = 0; i < str.Length; i++)
    {
        newstr += str.ToLower()[i];
    }
    Console.Write("Новая строка: ");
    Console.Write(newstr);
  }
}
