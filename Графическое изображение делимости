sing System;

class Program {
    static void Main(string[] args) 
      {
      int n = Convert.ToInt32(Console.ReadLine());
      for ( int i = 1; i<=n; i++)
      {
        Console.Write(i);
        for ( int j = 1; j<=i;j++)
        {
          if( i%j == 0)
          {
            Console.Write("+");
          }
        }
      Console.WriteLine();
      }
    }
}
