using System;

namespace таблица_умножения
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Таблица умножения");
            for (int i = 1; i < 10; i++)
            {
                for (int j = 1; j < 7 ; j++)
                {
                    Console.Write($"{j}*{i}={j * i} \t");
                }
                Console.WriteLine();
            }

        }
    }
}
