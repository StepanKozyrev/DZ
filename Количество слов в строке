using System;
class HelloWorld {
  static void Main() {
    Console.Write("Введите строку: ");
    string str = Console.ReadLine();
    int count = 0;
    for (int i = 0; i < str.Length; i++)
    {
      if (str[i] == ' ')
      {
        count++;
      }
    }
    Console.Write("Количество слов: ");
    Console.Write(count+1);
  }
} 
